// Some comparison statements

    .section .text, "ax"

// > expressions (0, 1, 0)
    .byte 0, 1, 0
// < expressions (1, 0, 0)
    .byte 1, 0, 0
// >= expressions (0, 1, 1)
    .byte 0, 1, 1
// <= expressions (1, 0, 1)
    .byte 1, 0, 1
// == expressions (1, 0)
    .byte 1, 0
// != expressions (0, 1)
    .byte 0, 1

// not 1>2
    .if 0
        .asciz "1>2"
    .else
        .asciz "NOT 1>2"
    .endif

// 1<2
    .if 1
        .asciz "1<2"
    .else
        .asciz "NOT 1<2"
    .endif


    .end

