// Test some of the builtins

        .section .text, "ax"

// AREA: (not objasm, armasm only)
        .ascii ".text"

// Filename (not objasm, armasm only)
        .ascii "objasm/variables/builtins.s"

// Line number (twice, to see it change) (not objasm, armasm only)
        .balign
        .word     14
        .word     15

.set here, 23

// Mapping should be 24
        .byte 24

// Config should be 32
        .byte 32

// Architecture
        .byte 7

        .end

