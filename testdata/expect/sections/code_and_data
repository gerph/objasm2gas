
// simple code region
        .section .text, "ax"

        MOV     pc, lr

// C-linkable area (should just be .text)
        .section .text, "ax"

        MOV     pc, lr

// C-linkable data area
        .section .data, "aw", %progbits

        .word     42

// C-linkable readonly data
        .section .rodata, "a", %progbits

        .asciz "Hello world"

